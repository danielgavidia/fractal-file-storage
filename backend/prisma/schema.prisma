generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  // Defaults
  id        String   @id @unique @default(uuid()) // or cuid
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  // Fields
  fileKey String @unique
}
